{
    "package": {
        "name": "AnVerGen",
        "repo": "maven",
        "subject": "no-creativity",
        "desc": "An automatic version generator for Android applications.",
        "website_url": "https://github.com/no-creativity/AnVerGen",
        "issue_tracker_url": "https://github.com/bintray/bintray-client-java/issues",
        "vcs_url": "https://github.com/bintray/bintray-client-java.git",
        "github_use_tag_release_notes": true,
        "github_release_notes_file": "RELEASE_NOTE.md"
    },
    "version": {
        "name": "0.1",
        "desc": "This is a pre-release version and not recommended to use.",
        "released": "2016-10-01",
        "vcs_tag": "0.1",
        "attributes": [
            {
                "gradle-plugin": "org.no_creativity.anvergen:org.no_creativity.anvergen:anvergen"
            }
        ],
        "gpgSign": false
    },
    "files": [
        {
            "includePattern": "build/libs/(.*\.jar)",
            "uploadPattern": "org/no_creativity/anvergen/0.1/$1"
        }
    ],
    "publish": true
}
